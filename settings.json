{
	"files.autoSave": "afterDelay",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"editor.minimap.enabled": false,
	"symbols.hidesExplorerArrows": false,
	"liveServer.settings.donotShowInfoMsg": true,
	"editor.fontFamily": "Geist Mono Light",
	"editor.fontSize": 20,
	"editor.lineHeight": 1.75,
	"workbench.startupEditor": "newUntitledFile",
	"editor.renderLineHighlight": "none",
	"workbench.editor.labelFormat": "short",
	"editor.fontLigatures": true,
	"explorer.compactFolders": false,
	"breadcrumbs.enabled": false,
	"workbench.iconTheme": "symbols",
	"code-runner.runInTerminal": true,
	"git.autofetch": true,
	"apc.electron": {
		"titleBarStyle": "hidden",
		"frame": false,
	},
	"workbench.layoutControl.enabled": false,
	"apc.header": {
		"height": 33,
	},
	"apc.font.family": "Victor Mono",
	"apc.stylesheet": {
		"body" : {
			"font-size": "20px"
		},
		".title-label > h2": "display: none",
		".editor-actions": "display: none",
		".pane-body": "padding: 8px",
		".pane-header": "padding: 0 8px",
		".nosidebar .inline-tabs-placeholder": "width: 75px",
		".split-view-view:firs-child .pane-header": "display: none !important",
		".monaco-list-row": "border-radius: 4px;",
		".monaco-workbench": "--vscode-editor-background: #141414; --vscode-editorGutter-background: #141414;",
		"monaco-workbench .monaco-list:not(.element-focused):focus:before": "display: none",
		".custom-list-row .monaco-list-row": "font-size: 17px",
		".monaco-sash.horizontal.maximum": "display: none",
		".part.statusbar.status-border-top": "--status-border-top-color: #141414 !important; background-color: #141414 !important",
		".monaco-sash.vertical": "background-color: #141414 !important;",
		".title.tabs.show-file-icons": "background-color: #141414 !important",
		".line-numbers": "color: #ff8a8a33 !important",
		".line-numbers.active-line-number": "color: #ff8a8a !important",
		".sticky-line-number-inner": "color: #6f3e5adb !important",
		".empty": "background-color: #141414 !important",
		".label-name": "font-size: 16px !important",
		".mtk11": "color: #ff8a8a !important",
		".mtk9": "color: #f1f991 !important",
		".mtk5": "color: #3ae4ed !important"

	},
	"workbench.editor.enablePreview": false,
	"apc.listRow": {
		"height": 30
	},
	"workbench.activityBar.location": "hidden",
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"README.md": "LICENSE*, package.json*, .gitignore*, requirements*, Docker*, db.sqlite3*, .gitattributes*",
	},
	"terminal.integrated.fontSize": 16,
	"terminal.integrated.fontFamily": "JetBrains Mono",
	"window.commandCenter": false,
	"editor.mouseWheelZoom": true,
	"window.zoomLevel": -1.8,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"liveServer.settings.host": "localhost",
	"cmake.showOptionsMovedNotification": false,
	"editor.renderWhitespace": "none",
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.env.windows": {},
	"workbench.colorTheme": "Min Darkest Dracula",
	"window.menuBarVisibility": "compact",
	"editor.wordWrap": "on",
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.guides.indentation": false,
	"workbench.statusBar.visible": true,
}
